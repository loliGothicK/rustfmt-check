{
    "name": "rustfmt-check",
    "version": "0.5.0",
    "description": "\"GitHub Action for PR annotations with rustfmt checks\"",
    "main": "lib/src/main.js",
    "scripts": {
        "build": "tsc",
        "lint": "npx biome lint ./src",
        "lint:fix": "npx biome lint --write ./src",
        "fmt": "npx biome format --write ./src",
        "fmt:check": "npx biome format ./src",
        "ncc": "ncc build --minify --source-map --license LICENSE",
        "test": "jest --runInBand",
        "release": "yarn fmt && yarn lint:fix && yarn build && yarn ncc"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LoliGothick/rustfmt-check/issues"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/core-js": "2.5.8",
        "@types/node": "24.10.10",
        "@vercel/ncc": "0.38.4",
        "jest": "30.2.0",
        "ts-jest": "^29.2.5",
        "typescript": "5.9.3"
    },
    "dependencies": {
        "@actions/core": "2.0.3",
        "@actions/exec": "3.0.0",
        "@actions/github": "7.0.0",
        "neverthrow": "8.2.0",
        "outdent": "0.8.0",
        "string-argv": "0.3.2",
        "zod": "4.3.6"
    },
    "volta": {
        "node": "24.13.0",
        "yarn": "1.22.22"
    },
    "packageManager": "yarn@1.22.22"
}
